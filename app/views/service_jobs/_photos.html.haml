.sj-photo-attachments
  %h5
    = t(".title_#{division}")
  .mini-gallery
    = photo_gallery_mini(@service_job, division)
  .add-photo-btn
    = link_to t(".add_photo"),
      "#",
      id: "add-photo-btn-#{division}-#{@service_job.id}",
      class: "change-to-qr"
    .qr_code.hidden{data: {division: division.to_s}}
